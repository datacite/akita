{
  "github": {
    "enabled": false
  },
  "rewrites": [
    {
      "source": "/api/download-reports/:path*",
      "destination": "/download-reports/:path*"
    },
    {
      "source": "/api/app/(.*)",
      "destination": "/api/app"
    }
  ],
  "functions": {
     "src/app/api/download-reports/**/*": {
      "memory": 512,
      "maxDuration": 30
    },
    "src/pages/**/*": {
      "memory": 512,
      "maxDuration": 8
    },
    "src/app/ror.org/[rorid]/page.tsx": {
      "memory": 512,
      "maxDuration": 15
    },
    "src/app/**/*": {
      "memory": 512,
      "maxDuration": 8
    }
  }
}
